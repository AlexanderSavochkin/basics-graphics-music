LOCATE    COMP "CLK" SITE "P6"; // 25MHz crystal oscillator
IOBUF     PORT "CLK" IO_TYPE=LVCMOS33;
FREQUENCY PORT "CLK" 25 MHZ;

LOCATE COMP "LED[0]" SITE "T6";
IOBUF  PORT "LED[0]" IO_TYPE=LVCMOS33;

LOCATE COMP "KEY[0]" SITE "R7";
IOBUF  PORT "KEY[0]" IO_TYPE=LVCMOS33;

LOCATE COMP "GPIO[0]" SITE "R12";
IOBUF  PORT "GPIO[0]" IO_TYPE=LVCMOS33;
LOCATE COMP "GPIO[1]" SITE "T14";
IOBUF  PORT "GPIO[1]" IO_TYPE=LVCMOS33;

LOCATE COMP "HDMI_P[0]" SITE "G19";
