LOCATE COMP "HDMI_N[1]" SITE "F19";
LOCATE COMP "HDMI_N[2]" SITE "D19";
LOCATE COMP "HDMI_N[3]" SITE "K19";

IOBUF  PORT "HDMI_P[0]" IO_TYPE=LVCMOS33;
